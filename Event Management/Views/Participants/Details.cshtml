@model Event_Management.Models.Participant

@{
    ViewData["Title"] = "Event Details";
}

<h1>Participant Details</h1>

<div>
    <h4>Event</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Model.Ad
        </dd>
        <dt class="col-sm-2">
            Soyad
        </dt>
        <dd class="col-sm-10">
            @Model.Soyad
        </dd>
        <dt class="col-sm-2">
            E_posta
        </dt>
        <dd class="col-sm-10">
            @Model.E_posta
        </dd>
        <dt class="col-sm-2">
            Status
        </dt>
        <dd class="col-sm-10">
            @Model.Status
        </dd>
    </dl>
</div>
<p>
    <a asp-action="Edit" asp-route-id="@Model.P_Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</p>
